{
  "version": "1.0.0",
  "name": "primer-product",
  "description": "Primer CSS's product modules",
  "homepage": "http://primercss.io/",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "style": "build/build.css",
  "files": [
    "index.scss",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/primer-product.git"
  },
  "bugs": {
    "url": "https://github.com/primer/primer-product/issues"
  },
  "scripts": {
    "setup": "if [ ! -d \"node_modules\" ]; then npm install; fi",
    "build": "primer-module-build index.scss",
    "prepublish": "npm run setup && npm run build",
    "test": "npm run build"
  },
  "dependencies": {
    "primer-alerts": "*",
    "primer-avatars": "*",
    "primer-blankslate": "*",
    "primer-markdown": "*",
    "primer-states": "*",
    "primer-support": "*"
  },
  "peerDependencies": {
    "primer-core": "*"
  },
  "devDependencies": {
    "primer-module-build": "*"
  }
}
